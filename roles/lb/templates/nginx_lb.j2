upstream application {  
    server 192.168.56.101;
    server 192.168.56.102;
}

server {  
    listen  80;

    location / {
        proxy_set_header  Host             $host;
        proxy_set_header  X-Real-IP        $remote_addr;
        proxy_set_header  X-Forwarded-For  $remote_addr;
        proxy_pass http://application;
    }
}

